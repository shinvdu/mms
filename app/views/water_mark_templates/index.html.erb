<%= render 'user_videos/nav' %>

<table>
  <thead>
  <tr>
    <th>水印名称</th>
    <th>Width</th>
    <th>Height</th>
    <th>位置</th>
    <th>内容</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @water_mark_templates.each do |water_mark_template| %>
      <tr>
        <td><%= water_mark_template.name %></td>
        <td><%= water_mark_template.width %></td>
        <td><%= water_mark_template.height %></td>
        <td><%= water_mark_template.refer_pos %></td>
        <td><%= water_mark_template.text %></td>
        <td><%= link_to 'Show', water_mark_template %></td>
        <td><%= link_to 'Edit', edit_water_mark_template_path(water_mark_template) %></td>
        <td><%= link_to 'Destroy', water_mark_template, method: :delete, data: {confirm: 'Are you sure?'} %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Water mark template', new_water_mark_template_path %>
