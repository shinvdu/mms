<h2>Log in</h2>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
       <p><%= f.label :email %><br />
    <%= f.text_field :email %></p>
  </div>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>
  <div>
    <%= link_to "QQ账号登录",  account_omniauth_authorize_path(:tqq) %>  |
    <%= link_to "豆瓣账号登录",  account_omniauth_authorize_path(:douban) %> |
    <%= link_to "微博账号登录",  account_omniauth_authorize_path(:weibo) %>
  </div>

<%= render "devise/shared/links" %>
