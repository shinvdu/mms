<p id="notice"><%= notice %></p>

<h1>Listing Transcoding Strategies</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th>Data</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transcoding_strategies.each do |transcoding_strategy| %>
      <tr>
        <td><%= transcoding_strategy.name %></td>
        <td><%= transcoding_strategy.user_id %></td>
        <td><%= transcoding_strategy.data %></td>
        <td><%= transcoding_strategy.note %></td>
        <td><%= link_to 'Show', transcoding_strategy %></td>
        <td><%= link_to 'Edit', edit_transcoding_strategy_path(transcoding_strategy) %></td>
        <td><%= link_to 'Destroy', transcoding_strategy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transcoding strategy', new_transcoding_strategy_path %>
