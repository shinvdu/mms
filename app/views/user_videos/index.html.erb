<h1>UserVideos#index</h1>
<p>Find me in app/views/user_videos/index.html.erb</p>


<%= link_to 'upload video', new_user_video_path %>
<table>
    <thead>
        <tr>
            <th>id</th>
            <th>file name</th>
            <th>status</th>
            <th>edit</th>
            <th>remove</th>
            <th>mini video url</th>
        </tr>
    </thead>
    <tbody>
        <% @videos.each do |video| %>
        <tr>
            <td><%= video.id %></td>
            <td><%= video.file_name %></td>
            <td><%= video.status %></td>
            <td><%= link_to 'e', user_video_path(video) %></td>
            <td><%= button_to 'x', user_video_path(video), :method => :delete %></td>
            <td><%= make_url 'preview', video %></td>
        </tr>
        <% end %>
    </tbody>
</table>
