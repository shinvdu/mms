<p id="notice"><%= notice %></p>

<h1>Listing Tags Relationships</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>User video</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tags_relationships.each do |tags_relationship| %>
      <tr>
        <td><%= tags_relationship.tag_id %></td>
        <td><%= tags_relationship.user_video_id %></td>
        <td><%= link_to 'Show', tags_relationship %></td>
        <td><%= link_to 'Edit', edit_tags_relationship_path(tags_relationship) %></td>
        <td><%= link_to 'Destroy', tags_relationship, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tags relationship', new_tags_relationship_path %>
